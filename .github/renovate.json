{ 
    "enabled": true,
    "docker": {
        "pinDigests": true,
        "enabled": true
    },
    "docker-compose": {
        "digest": {
            "enabled": false
        }
    },
    "packageRules": [
        {
          "automerge": true,
          "matchPaths": ["Dockerfile", ".github/workflows/*.yml"]
        }
    ],
    "ignorePaths": ["docker-compose.yml"]
}
